<h2>Product List</h2>
<div class="pull-right">
  <%= link_to("Add new product", new_admin_product_path, :class => "btn btn-default") %>
</div>
<table class="table table-bordered">
  <% @products.each do |p| %>
    <tr>
      <td>
        <%= link_to(p.title, admin_product_path(p)) %>
      </td>
      <td>
        <%= p.description %>
      </td>
      <td>
        <%= p.created_at %>
      </td>
    </tr>
  <% end %>
</table>
